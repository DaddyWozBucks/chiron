<div class="col-xs-12 col-md-8 col-md-off-set-2 workout_page">
	<ul>
		<div class="panel panel-header"><%= @worktemp.name %></div>
		<% @exercises.each do |ex| %>
			<% if ex.weight == nil %>
				<li><%= ex.name %> Distance:<%=ex.distance%> Time: <%=ex.time%> Calories: <%=ex.calories%> </li>
				<% else %>
				<li><%= ex.name %> Weight:<%=ex.weight%> Reps: <%=ex.reps%> Sets: <%=ex.sets%> </li>
			<% end %>
		<% end %>
	</ul>
</div>